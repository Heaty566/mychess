{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start:dev": "NODE_ENV=development next dev",
        "format": "prettier --write \"./**/*.{js,ts,jsx,tsx}\" ",
        "build": "cross-env GENERATE_SOURCEMAP=false next build",
        "start": "NODE_ENV=production next start",
        "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
        "test:e2e": "jest --findRelatedTests /test/pagesE2E/ --runInBand --detectOpenHandles --forceExit",
        "test:watch": "NODE_ENV=test  jest --watch --runInBand --detectOpenHandles",
        "test:cov": "NODE_ENV=test  jest --coverage --runInBand --detectOpenHandles --forceExit"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.5.1",
        "axios": "^0.21.1",
        "global": "^4.4.0",
        "next": "10.1.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-google-recaptcha": "^2.1.0",
        "react-hook-form": "^7.0.0",
        "react-redux": "^7.2.3",
        "socket.io-client": "^2.4.0",
        "universal-cookie": "^4.0.4"
    },
    "devDependencies": {
        "@babel/core": "^7.13.14",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.12",
        "@types/react": "^17.0.3",
        "@types/react-google-recaptcha": "^2.1.0",
        "@types/socket.io-client": "^1.4.36",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
