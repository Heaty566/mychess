        "babel-jest": "^26.6.3",
        "babel-plugin-inline-react-svg": "^2.0.1",
        "dotenv": "^8.2.0",
        "eslint": "^7.23.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "identity-obj-proxy": "^3.0.0",
        "postcss": "^8.2.9",
        "prettier": "^2.2.1",
